{
  "compilerOptions": {
    "paths": {
      "@Src/*": ["./*"],
      "@Styles": ["Styles"],
      "@Style/*": ["Styles/*"],
      "@Hook/*": ["Hooks/*"],
      "@Hooks": ["Hooks"],
      "@Element/*": ["Components/Elements/*"],
      "@Elements": ["Components/Elements"],
      "@Layouts": ["Components/Layouts"],
      "@Pages": ["Pages"],
      "@Page/*": ["Pages/*"],
      "@Modules": ["Modules"],
      "@Module/*": ["Modules/*"],
      "@Stores": ["Store"],
      "@Store/*": ["Store/*"],
      "@Utils": ["Utils"],
      "@Util/*": ["Utils/*"],
      "@Types": ["Types"],
      "@Type/*": ["Types/*"],
      "@Helper": ["Utils/Helper"],
      "@API": ["Modules/API"],
      "@Assets": ["Assets"],
      "@Asset/*": ["Assets/*"],
      "@Services": ["Services"],
      "@Service/*": ["Services/*"]
    }
  }
}
